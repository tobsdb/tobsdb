{
  "name": "tobsdb",
  "version": "1.3.0",
  "description": "Nodejs Client for tobsdb",
  "main": "./dist/index.js",
  "author": "TobsDB",
  "license": "ISC",
  "scripts": {
    "start": "node ./dist/index.js",
    "test": "pnpm build && node --test --require ts-node/register ./tests/*.ts",
    "test:schema": "go run ../../../cmd/tdb-generate -path ./schema.tdb -out ./tests/schema.ts -lang ts",
    "build": "rollup -c",
    "npm-pub": "pnpm build && npm publish",
    "postinstall": "./scripts/postinstall.js"
  },
  "bin": {
    "tobsdb": "./scripts/tobsdb.js"
  },
  "repository": {
    "url": "https://github.com/tobsdb/tobsdb/blob/main/tools/client/js"
  },
  "keywords": [
    "tobsdb",
    "tobs database",
    "tdb",
    "db",
    "database"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@types/node": "^20.6.5",
    "esbuild": "^0.19.3",
    "rollup": "^3.29.3",
    "rollup-plugin-dts": "^6.0.2",
    "rollup-plugin-esbuild": "^6.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": "./dist/*"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "files": [
    "./dist/*",
    "./scripts/*"
  ]
}
